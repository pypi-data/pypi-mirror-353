{% raw %}{#def search_app_config #}
{%- extends config.BASE_TEMPLATE %}
{%- set title = _("Search results") ~ " | " ~ _("{% endraw %}{{cookiecutter.name}}{% raw %}") %}

{%- block javascript %}
    {{ super() }}
    {{ webpack['{% endraw %}{{cookiecutter.name}}{% raw %}_search.js'] }}
{%- endblock %}

{%- block page_body %}
<div class="ui main container">
  <div data-invenio-search-config='{{ search_app_config | tojson }}'></div>
</div>
{%- endblock page_body %}
{% endraw %}
