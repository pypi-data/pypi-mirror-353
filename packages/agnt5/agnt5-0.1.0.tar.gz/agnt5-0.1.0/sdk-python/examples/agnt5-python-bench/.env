AGNT5_SERVICE_NAME=agnt5-python-bench-dev
AGNT5_SERVICE_VERSION=dev
AGNT5_RUNTIME_ENDPOINT=http://localhost:8081
AGNT5_LOG_LEVEL=DEBUG
AGNT5_DEV_MODE=true
AGNT5_HOT_RELOAD=true
AGNT5_ENV=development

# Telemetry Configuration
AGNT5_OTEL_ENDPOINT=opentelemetry-collector:4317
AGNT5_ENABLE_TRACING=true
AGNT5_ENABLE_METRICS=true
      
# AI Provider API Keys (for testing, optional)
ANTHROPIC_API_KEY=${ANTHROPIC_API_KEY:-}
OPENAI_API_KEY=${OPENAI_API_KEY:-}
GOOGLE_API_KEY=${GOOGLE_API_KEY:-}
MISTRAL_API_KEY=${MISTRAL_API_KEY:-}
TOGETHER_API_KEY=${TOGETHER_API_KEY:-}